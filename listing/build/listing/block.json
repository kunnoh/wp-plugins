{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/listing",
  "version": "0.1.0",
  "title": "Listing",
  "category": "widgets",
  "description": "Dynamic columns for listing",
  "example": {},
  "supports": {
    "color": {
      "background": false,
      "text": true
    },
    "html": false,
    "typography": {
      "fontSize": true
    }
  },
  "textdomain": "listing",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "attributes": {
    "itemsPerPage": {
      "type": "number",
      "default": 8
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    },
    "order": {
      "type": "string",
      "default": "desc"
    }
  }
}