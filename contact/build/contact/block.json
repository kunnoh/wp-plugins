{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/contact",
  "version": "0.1.0",
  "title": "Contact",
  "category": "widgets",
  "icon": "smiley",
  "description": "Contact section with schema markup, contact form, and business information.",
  "keywords": [
    "contact",
    "form",
    "business"
  ],
  "supports": {
    "html": false,
    "anchor": true
  },
  "textdomain": "contact",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "companyName": {
      "type": "string",
      "default": "Your Company Name"
    },
    "address": {
      "type": "object",
      "default": {
        "street": "123 Business Street",
        "city": "Your City",
        "state": "State",
        "zipCode": "12345",
        "country": "Country"
      }
    },
    "phone": {
      "type": "string",
      "default": "+1 (234) 567-890"
    },
    "email": {
      "type": "string",
      "default": "info@yourcompany.com"
    },
    "businessHours": {
      "type": "object",
      "default": {
        "weekdays": "Monday - Friday: 9:00 AM - 5:00 PM",
        "saturday": "Saturday: 10:00 AM - 2:00 PM",
        "sunday": "Sunday: Closed"
      }
    },
    "socialLinks": {
      "type": "object",
      "default": {
        "facebook": "",
        "twitter": "",
        "linkedin": "",
        "instagram": ""
      }
    },
    "showMap": {
      "type": "boolean",
      "default": true
    },
    "mapEmbedCode": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "textColor": {
      "type": "string",
      "default": "#333333"
    }
  }
}